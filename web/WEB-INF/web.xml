<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>GU.profile.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>GU.profile.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetPasswordServlet</servlet-name>
        <servlet-class>GU.profile.SetPasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForgotPasswordServlet</servlet-name>
        <servlet-class>GU.profile.ForgotPasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactServlet</servlet-name>
        <servlet-class>GU.contact.ContactServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>GU.profile.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TranscriptServlet</servlet-name>
        <servlet-class>GU.student.TranscriptServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProgressTrackerServlet</servlet-name>
        <servlet-class>GU.student.ProgressTrackerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RetakeServlet</servlet-name>
        <servlet-class>GU.student.RetakeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudySuspensionServlet</servlet-name>
        <servlet-class>GU.applications.StudySuspensionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudyContinuationServlet</servlet-name>
        <servlet-class>GU.applications.StudyContinuationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RetakeApplicationServlet</servlet-name>
        <servlet-class>GU.applications.RetakeApplicationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThesisTraining1Servlet</servlet-name>
        <servlet-class>GU.student.ThesisTraining1Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThesisTraining2Servlet</servlet-name>
        <servlet-class>GU.student.ThesisTraining2Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinalReportServlet</servlet-name>
        <servlet-class>GU.student.FinalReportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CourseRemarkingServlet</servlet-name>
        <servlet-class>GU.applications.CourseRemarkingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GPCalculatorServlet</servlet-name>
        <servlet-class>GU.tools.GPCalculatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewUserServlet</servlet-name>
        <servlet-class>GU.admin.NewUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetPasswordServlet</servlet-name>
        <url-pattern>/setPassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgotPasswordServlet</servlet-name>
        <url-pattern>/validateEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgotPasswordServlet</servlet-name>
        <url-pattern>/validateOtp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactServlet</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/student/modifyInformation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TranscriptServlet</servlet-name>
        <url-pattern>/academicTranscript</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProgressTrackerServlet</servlet-name>
        <url-pattern>/student/generateInvoice</url-pattern>
        <url-pattern>/student/feeStatement</url-pattern>
        <url-pattern>/student/resultOverview</url-pattern>
        <url-pattern>/student/profileCheck</url-pattern>
        <url-pattern>/student/graduationProject</url-pattern>
        <url-pattern>/student/officialDocument</url-pattern>
        <url-pattern>/student/projectSurvey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RetakeServlet</servlet-name>
        <url-pattern>/applications/uploadRetakeReceipt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CourseRemarkingServlet</servlet-name>
        <url-pattern>/applications/courseRemarking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudySuspensionServlet</servlet-name>
        <url-pattern>/applications/suspension</url-pattern>
        <url-pattern>/applications/suspensionEvidence</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudyContinuationServlet</servlet-name>
        <url-pattern>/student/studyContinuation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RetakeApplicationServlet</servlet-name>
        <url-pattern>/applications/submitApplication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThesisTraining1Servlet</servlet-name>
        <url-pattern>/student/thesisTraining1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThesisTraining2Servlet</servlet-name>
        <url-pattern>/student/thesisTraining2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FinalReportServlet</servlet-name>
        <url-pattern>/student/finalReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GPCalculatorServlet</servlet-name>
        <url-pattern>/tools/testing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewUserServlet</servlet-name>
        <url-pattern>/admin/newStudent</url-pattern>
        <url-pattern>/admin/newInstructor</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
